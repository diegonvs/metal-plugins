      "metal-jsx"
    ]
  },
  "dependencies": {
    "metal-jsx": "^1.0.0-rc.15",
    "metal-state-validators": "^0.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-metal-jsx": "0.0.2",
    "gulp": "^3.8.11",
    "gulp-metal": "^1.0.0"
  }
}
