{
  "name": "metal-position",
  "description": "Metal.js position helps you to best position a DOM element into the screen. Useful to build tooltips, dropdowns an much more.",
  "version": "2.1.1",
  "author": {
    "name": "Eduardo Lundgren",
    "email": "edu@rdo.io"
  },
  "dependencies": {
    "metal": "^2.16.5",
    "metal-dom": "^2.16.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-loader": "^8.0.0",
    "babel-plugin-transform-node-env-inline": "^0.4.0",
    "browserslist-config-clay": "^2.3.2",
    "webpack": "^3.11.0"
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "files": [
    "lib",
    "src/**/*.js",
    "test/**/*.js"
  ],
  "jsnext:main": "src/all/position.js",
  "keywords": [
    "metal"
  ],
  "license": "BSD",
  "main": "lib/all/position.js",
  "repository": "metal/metal-position",
  "scripts": {
    "compile": "babel --presets=@babel/preset-env -d lib/ src/",
    "prepublish": "npm run compile"
  },
  "browserslist": [
    "extends browserslist-config-clay"
  ]
}
