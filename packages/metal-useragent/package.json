{
  "name": "metal-useragent",
  "description": "Metal.js browser user agent detection.",
  "version": "3.0.0",
  "browserslist": [
    "extends browserslist-config-clay"
  ],
  "dependencies": {
    "metal": "^2.16.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "browserslist-config-clay": "^2.3.2",
    "webpack": "^3.11.0"
  },
  "engines": {
    "node": ">=0.12.0",
    "npm": ">=3.0.0"
  },
  "files": [
    "lib",
    "src",
    "test"
  ],
  "jsnext:main": "src/UA.js",
  "keywords": [
    "metal"
  ],
  "license": "BSD",
  "main": "lib/UA.js",
  "repository": "metal/metal-useragent",
  "scripts": {
    "build": "webpack",
    "compile": "babel --presets=@babel/preset-env -d lib/ src/",
    "prepublish": "npm run compile",
    "test": "gulp test"
  }
}
