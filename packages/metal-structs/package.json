        "chai": "^4.1.2",
        "karma": "^1.1.0",
        "karma-chai": "^0.1.0",
        "karma-chrome-launcher": "^2.2.0",
        "karma-mocha": "^1.3.0",
        "karma-sauce-launcher": "^1.2.0",
        "karma-webpack": "^2.0.5",
        "mocha": "^4.0.1",
