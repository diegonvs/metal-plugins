{
  "name": "metal-css-transitions",
  "version": "0.0.0",
  "description": "Metal component used to apply css transitions",
  "license": "BSD",
  "repository": "bryceosterhaus/metal-css-transitions",
  "engines": {
    "node": ">=0.12.0",
    "npm": ">=3.0.0"
  },
  "main": "lib/TransitionWrapper.js",
  "jsnext:main": "src/TransitionWrapper.js",
  "files": [
    "lib",
    "src",
    "test"
  ],
  "scripts": {
    "build": "gulp build",
