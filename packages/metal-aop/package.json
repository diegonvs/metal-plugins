{
  "name": "metal-aop",
  "description": "Metal.js utility for AOP.",
  "version": "3.0.0",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-loader": "^8.0.0",
    "browserslist-config-clay": "^2.3.2",
    "lint-staged": "^5.0.0",
    "webpack": "^3.11.0"
  },
  "engines": {
    "node": ">=0.12.0",
    "npm": ">=3.0.0"
  },
  "files": [
    "lib",
    "src",
    "test"
  ],
  "jsnext:main": "src/AOP.js",
  "keywords": [
    "metal"
  ],
  "license": "BSD",
  "lint-staged": {
    "*.js": [
      "npm run format",
      "git add"
    ]
  },
  "main": "lib/AOP.js",
  "repository": "https://github.com/metal/metal-plugins/tree/master/packages/metal-aop",
  "scripts": {
    "compile": "babel --presets=@babel/preset-env -d lib/ src/",
    "precommit": "lint-staged",
    "prepublish": "npm run compile"
  },
  "browserslist": [
    "extends browserslist-config-clay"
  ]
}
